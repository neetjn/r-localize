{
  "name": "r-localize",
  "version": "0.0.2",
  "description": "Localization mixin and tag for Riot.js based off of Vue.js v-localize project.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/node": "^9.4.0",
    "@types/webpack": "^3.8.4",
    "electron": "^1.7.11",
    "jasmine": "^2.9.0",
    "karma": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.2.2",
    "karma-jasmine": "^1.1.1",
    "karma-riot": "^2.0.0",
    "riot-typescript": "^1.0.10",
    "ts-loader": "^3.3.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "bundle": "node_modules/.bin/webpack --config webpack.config.ts",
    "lint": "node_modules/.bin/tslint --project tsconfig.json",
    "test": "node_modules/.bin/karma start test/karma.conf.js",
    "build": "npm run lint && npm run bundle && npm run test"
  },
  "repository": {
    "type": "git",
